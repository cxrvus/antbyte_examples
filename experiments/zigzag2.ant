set width = 256;
set height = 128;
set tpf = 4;
set fps = 32;
set start = top_left;
set border = cycle;
# set border = die;

fn spike = () => () {
	CC = 1;
	C1 = -C1;
	# C3 = T7;

	D1 = T2;
	D2 = T3;
	D0 = T7;
}

fn reproduce = () => spawn {
	spawn = and(T7, -or(
		T3,
		T2,
		T1,
		# T0,
		0
	));
	AX = and(T7, T4);
}

ant clone2 = 4 {
	() = spike();
}

ant clone = 2 {
	() = spike();
	A2 = reproduce();
}

ant main {
	() = spike();
	A1 = reproduce();
}
