set size = 128;
set start = top_left;
set tpf = 128;
set fps = 32;
set border = wrap;
set colors = bin;

fn dir_change = (mem, clock) => (mem_out, dir) {
	dir = xor(mem, clock);
	mem_out = clock;
}

ant main {
	CC = 1;
	C0 = -C0;

	(M2, D1) = dir_change(M2, T2);
	(M3, D2) = dir_change(M3, T3);
	(M7, D0) = dir_change(M7, T7);
}
